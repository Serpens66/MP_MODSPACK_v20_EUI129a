<GameData>
	<!-- should still be very good, but not utterly overpowered, by serp 
    
    AIGrowthPercent ( AI braucht nur diese prozentzahl an Nahrung für einen neuen Bürger in einer stadt ) 
    AITrainPercent  (prozent einheiten prodktionskosten)
    AIConstructPercent  (prozent gebäude prduktionskosten)
    AICreatePercent (prozent projekte produkionkosten)
    AIUnitCostPercent (prozent maintenance kosten)
    AIUnitSupplyPercent (maximal anzahl an einheiten die man haben "kann", wobei ein überschreiten food und production um 5% por unit reduziertrt, bis zu 75%. Wert wird durch zahl der städte/Bürger und Gebäude darin erhöht)
    AIPerEraModifier ( wert wird mit zeitalternummer multipliziert und soviel prozent "teurer" (bei negativem wert günstiger) werden ALLE Produktionskosten (einheiten, gebäude, projekte) und auch AIGrowthPercent)
    
    -->
    <HandicapInfos>
        <Update>
			<Where Type="HANDICAP_AI_DEFAULT"/>
			<Set AIUnitSupplyPercent="-15" AIUnitCostPercent="90" AITrainPercent="100" AIConstructPercent="100" AICreatePercent="100" AIBuildingCostPercent="100" AIGrowthPercent="50" AIPerEraModifier="0" />
		</Update>
        <Update>
			<Where Type="HANDICAP_SETTLER"/>
