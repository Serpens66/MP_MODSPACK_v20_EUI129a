    <!-- increase from 30. the amount of production one can stockpile with the stockpile mod per era. In sql will also be a trigger, so mod load order does not matter -->
    <CustomModOptions>
        <Update>
			<Set Value="60"/> 
			<Where Name="PROCESS_STOCKPILE_PER_ERA"/>
		</Update>
    </CustomModOptions>
        
    
	<Processes>
		<Update>
			<Set TechPrereq="TECH_MINING"/>
			<Where Type="PROCESS_WEALTH"/>
		</Update>
        <Update>
			<Set TechPrereq="TECH_WRITING"/>
			<Where Type="PROCESS_RESEARCH"/>
		</Update>
	</Processes>
	
    <!-- increase from 25% -->
    <Process_ProductionYields>
		<Update>
			<Set Yield="35"/>
			<Where ProcessType="PROCESS_WEALTH"/>
		</Update>
        <Update>
			<Set Yield="35"/>
			<Where ProcessType="PROCESS_RESEARCH"/>
		</Update>
        <!-- increase from 80. the percentage conversion of production for stockpile mod In sql will also be a trigger, so mod load order does not matter-->
		<Update>
			<Set Yield="95"/>
			<Where ProcessType="PROCESS_STOCKPILE"/>
		</Update>
	</Process_ProductionYields>
    
    <!-- maximal science modifier, vermutlich in prozent, den man bekommt wenn alle anderen civs die tech schon erforscht haben (und man diese bereits getroffen hat), erhoet von 30 -->
    <Defines>
        <Update>
			<Set Value="40"/> 
			<Where Name="TECH_COST_TOTAL_KNOWN_TEAM_MODIFIER"/>
		</Update>
    </Defines>
    
